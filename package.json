{
  "name": "brocks-patterson-resume",
  "version": "2.0.0",
  "description": "brock's-resume",
  "author": "brock patterson",
  "license": "UNLICENSED",
  "homepage": "https://github.com/brockcp/resume#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brockcp/resume.git"
  },
  "scripts": {
    "import-mfizz-fonts": "copyfiles -u 3 node_modules/font-mfizz/dist/font-mfizz.eot  node_modules/font-mfizz/dist/font-mfizz.svg node_modules/font-mfizz/dist/font-mfizz.ttf node_modules/font-mfizz/dist/font-mfizz.woff src/fonts",
    "import-fontawesome-fonts": "copyfiles -u 2 node_modules/font-awesome/fonts/* src",
    "import-jquery": "copyfiles -u 3 'node_modules/jquery/dist/jquery.min.js' src/js",
    "sass-css": "node-sass --output-style nested --indent-type tab --indent-width 1 -o src/css src/scss",
    "sass-css-compress": "node-sass --output-style compressed -o src/css src/scss",
    "dev-s": "browser-sync start --server --files 'src/*' --startPath 'src'",
    "prod-html-css": "useref src/index.html docs/index.html --css concat",
    "prod-img": "copyfiles -u 2 src/img/* docs/img",
    "prod-js": "copyfiles -u 1 src/js/**/*.* docs",
    "prod-fonts": "copyfiles -u 1 src/fonts/*.* docs",
    "prod-s": "browser-sync start --server --files 'docs/*' --startPath 'docs'",
    "watch-scss": "onchange 'src/scss' -- npm-run-all sass-css",
    "XXXautoprefixer": "postcss -u autoprefixer -r docs/test/*"

  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "bootstrap": "^4.1.0",
    "browser-sync": "^2.23.6",
    "concat": "^1.0.3",
    "copyfiles": "^1.2.0",
    "font-awesome": "^4.7.0",
    "font-mfizz": "^2.4.1",
    "imagemin": "^5.3.1",
    "jquery": "^3.3.1",
    "node-sass": "^4.8.3",
    "popper.js": "^1.14.3",
    "uglify-js": "^3.3.21",
    "useref-file": "^0.2.1"
  },
  "dependencies": {
    "npm-run-all": "^4.1.3",
    "onchange": "^4.0.0"
  }
}
